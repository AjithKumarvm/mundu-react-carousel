{
  "name": "mundu-react-carousel",
  "version": "0.0.18",
  "description": "A Lightweight carousel for React",
  "main": "lib/index",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.1",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^4.6.0",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "^3.1.3"
  },
  "author": "Ajith Kumar VM <ajithkumarvm@gmail.com>",
  "homepage": "https://github.com/AjithKumarvm/mundu-react-carousel",
  "repository": {
    "type": "git",
    "url": "https://github.com/AjithKumarvm/mundu-react-carousel.git"
  },
  "dependencies": {},
  "keywords": [
    "react mundu carousel",
    "react carousel",
    "lightweight react carousel",
    "carousel for react",
    "pure react carousel",
    "simple react carousel",
    "react slider",
    "mundu carousel"
  ]
}
